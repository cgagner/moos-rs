  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'moos-core'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=moos-core"
        ],
        "filter": {
          "name": "moos-core",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'example-moos-app'",
